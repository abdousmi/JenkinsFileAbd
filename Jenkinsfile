    }
    }
    stage('Buildimage') {
		echo 'Build image';
    }
    stage('Testimage') {
		echo 'Test image';
    }
    stage('Pushimage') {
		echo 'Push image';
    }
    stage('Pullimage') {
		echo 'Pull image';
    }
}
